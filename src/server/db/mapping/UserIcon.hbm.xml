<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="common.db.entity">
  <class name="UserIcon" table="USER_ICONS">
    <id column="UIC_ID" name="id" type="integer">
      <generator class="increment"/>
    </id>
    
    <property column="UIC_UAC_ID" name="uacId" type="integer"/>       
    <property column="UIC_ICON_DATA" name="iconData" type="binary" length="1000000"/>

  </class>
</hibernate-mapping>
